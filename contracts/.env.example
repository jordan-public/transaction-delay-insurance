# Deployment Configuration
# Copy this file to .env and fill in your values

# Required: Private key for contract deployment
PRIVATE_KEY=your_private_key_here

# Required: Address of your RPC proxy service
# Use 0x0000000000000000000000000000000000000000 if not available yet
RPC_PROXY_ADDRESS=your_rpc_proxy_address_here

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Network RPC URLs (already configured in deploy.sh)
LOCAL_RPC_URL=http://localhost:8545
ZIRCUIT_RPC_URL=https://garfield-testnet.zircuit.com/
FLOW_RPC_URL=https://testnet.evm.flow.org
HEDERA_RPC_URL=https://testnet.hashio.io/api
